def fib(n) {
    let cond = n == 0 in 
    if cond then 0 else
        let cond = n == 1 in
        if cond then 1 else
            let a = n - 1 in
            let b = n - 2 in
            let v1 = fib(a) in
            let v2 = fib(b) in
            v1 + v2
}

def main() {
    let arg = ~readInt() in
    let res = fib(arg) in
    let _ = ~printInt(res) in
    0
}